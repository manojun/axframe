{
  "ts-node": {
    "compilerOptions": {
      "module": "commonjs"
    }
  },
  "compilerOptions": {
    "lib": ["dom", "es2015", "es2016", "es2017", "es2018", "esnext"],
    "target": "es2019",
    "module": "es2015",
    "moduleResolution": "node",
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "noImplicitAny": false,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "jsx": "react",
    "typeRoots": ["@types", "node_modules/@types"],
    "baseUrl": "src",
  },
  "exclude": ["node_modules"]
}
